@model IEnumerable<MvcOnlineTicariOtomasyon2.Models.Siniflar.SinifGrup3>

<div class="card">
    <div class="card-header">Ürün Marka Tablosu</div>
</div>
<div class="card-body p-0">
    <table class="table table-striped">
        <thead>
            <tr>
                <th style="width: 10px">#</th>
                <th>Marka</th>
                <th>Progress</th>
                <th style="width: 40px">Toplam</th>
            </tr>
        </thead>
        <tbody>
            @{ int sirano = 0; }
            @foreach (var x in Model)
            {
<tr>
    <td>@{sirano++;} @sirano</td>
    <td>@x.marka</td>
    <td>
        <div class="progress progress-xs">
            <div class="progress-bar progress-bar-danger" style="width: 55%"></div>
        </div>
    </td>
    <td><span class="badge bg-danger">@x.sayi</span></td>
</tr>
       }
        </tbody>
    </table>
</div>


